[core]
	excludesfile = /home/travis/.gitignore
	editor = nvim
[branch]
	autosetuprebase = always
[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[mergetool "meld"]
	cmd = /usr/sbin/meld \"$LOCAL\" \"$MERGED\" \"$REMOTE\" --output \"$MERGED\"
[alias]
	up = pull --rebase --autostash
	d = difftool
[difftool]
	prompt = false
